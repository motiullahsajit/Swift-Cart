{
  "version": 2,
  "builds": [
    {
      "src": "app.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.ts"
    }
  ],
  "env": {
    "PORT": "4000",
    "MONGO_URI": "@MONGO_URI",
    "STRIPE_KEY": "@STRIPE_KEY"
  }
}
